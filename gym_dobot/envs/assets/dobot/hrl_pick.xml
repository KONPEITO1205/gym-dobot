				<geom pos="0 -0.165 0" class="wall"/>
				<geom pos="0 -0.198 0" class="wall"/>
		</body>
		
		<body name="horizontalwall1" pos="0.62 0.685 0.025">
				<geom pos="0 0 0" class="wall"/>
				<geom pos="0.033 0 0" class="wall"/>
				<geom pos="0.066 0 0" class="wall"/>
				<geom pos="0.099 0 0" class="wall"/>
				<geom pos="0.132 0 0" class="wall"/>
		</body>

		<body name="horizontalwall2" pos="0.98 0.685 0.025">
				<geom pos="0 0 0" class="wall"/>
				<geom pos="-0.033 0 0" class="wall"/>
				<geom pos="-0.066 0 0" class="wall"/>
				<geom pos="-0.099 0 0" class="wall"/>
				<geom pos="-0.132 0 0" class="wall"/>
		</body>


	</worldbody>


	

</mujoco>